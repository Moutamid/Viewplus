<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_view_youtube">

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.moutamid.viewplus.ui.notifications.SettingsFragment"
        android:label="Store"
        tools:layout="@layout/fragment_settings" />

    <fragment
        android:id="@+id/navigation_view_youtube"
        android:name="com.moutamid.viewplus.ui.dashboard.ViewYoutubeFragment"
        android:label="View"
        tools:layout="@layout/fragment_view_youtube" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.moutamid.viewplus.ui.home.HomeFragment"
        android:label="Campaign"
        tools:layout="@layout/fragment_home" />

</navigation>